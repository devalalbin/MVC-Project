@model IEnumerable<CityDb>
@{
    ViewData["Title"] = "Index";
}



<h1>Cities</h1>


<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Country Code</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.CountryId</td>
            </tr>
        }
    </tbody>
</table>