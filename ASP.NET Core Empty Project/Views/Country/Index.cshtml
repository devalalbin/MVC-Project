@model IEnumerable<CountryDb>
@{
    ViewData["Title"] = "Index";
}

<div class="container">
    <h1>Countries</h1>


    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                </tr>
            }
        </tbody>
    </table>
    </div>
