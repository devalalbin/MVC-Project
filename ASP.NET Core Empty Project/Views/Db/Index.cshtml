@model IEnumerable<PersonDb>
@{
    ViewData["Title"] = "Index";
}



<h1>People</h1>


<table class="table">
    <thead>
        <tr>
            <th>SSN</th>
            <th>Name</th>
            <th>PhoneNr</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
            {
        <tr>
            <td>@item.SSN</td>
            <td>@item.Name</td>
            <td>@item.PhoneNr</td>
        </tr>
            }
    </tbody>
</table>